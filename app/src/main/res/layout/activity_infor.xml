<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.hueic.hueicprojectdoanthanhnien.activity.InforActivity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvInforJoin"
        android:text="Thong tin bao nhieu sinh vien tham gia"
        android:textSize="24sp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvInforNotJoin"
        android:text="Thong tin bao nhieu sinh vien khong tham gia"
        android:layout_below="@+id/tvInforJoin"
        android:textSize="24sp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvChart"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Bieu do "
        android:textSize="30sp"/>


</RelativeLayout>
